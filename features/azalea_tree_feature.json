{
  "format_version": "1.13.0",
  "minecraft:tree_feature": {
    "description": {
      "identifier": "minecraft:azalea_tree_feature"
    },
    "acacia_trunk": {
      "trunk_width": 1,
      "trunk_height": {
        "base": 1,
        "intervals": [ 1 ],
        "min_height_for_canopy": 1
      },
      "trunk_block": {
        "name": "minecraft:air"
      },
      "trunk_lean": {
        "allow_diagonal_growth": false,
        "lean_height": {
          "range_min": 1,
          "range_max": 2
        },
        "lean_steps": {
          "range_min": 1,
          "range_max": 2
        },
        "lean_length": {
          "range_min": 1,
          "range_max": 2
        }
      }
    },
    "random_spread_canopy": {
      "canopy_height": 1,
      "canopy_radius": 1,
      "leaf_placement_attempts": 1,
      "leaf_blocks": [
        ["minecraft:air", 0],
        ["minecraft:air", 0]
      ]
    },
    "base_block": [
      "minecraft:dirt_with_roots"
    ],
    "may_grow_on": ["minecraft:bedrock"],
    "may_replace": ["minecraft:bedrock"],
    "may_grow_through": ["minecraft:bedrock"]
  }
}